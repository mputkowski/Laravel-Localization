{
    "name": "mputkowski/laravel-locale",
    "description": "Powerful Localization for Laravel 5",
    "keywords": ["laravel", "framework", "localization", "locale", "Laravel Localization", "Laravel-Localization", "Laravel Locale", "Laravel-Locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Putkowski",
            "email": "m.putkowski@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/cookie": "5.5.*|5.6.*",
        "illuminate/support": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "graham-campbell/testbench": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "mputkowski\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mputkowski\\Tests\\Localization\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mputkowski\\Localization\\ServiceProvider"
            ],
            "aliases": {
                "Localization": "mputkowski\\Localization\\Facades\\Localization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
