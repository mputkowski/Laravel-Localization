{
    "name": "mputkowski/laravel-localization",
    "description": "Powerful Localization for Laravel",
    "keywords": ["laravel", "framework", "localization", "locale", "Laravel Localization", "Laravel-Localization", "Laravel Locale", "Laravel-Locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Putkowski",
            "email": "michal@putkow.ski"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "illuminate/cookie": "^6.0|^7.0|^8.0|^9.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5.8|^9.3.3",
        "graham-campbell/testbench": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "mputkowski\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mputkowski\\Tests\\Localization\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mputkowski\\Localization\\ServiceProvider"
            ],
            "aliases": {
                "Localization": "mputkowski\\Localization\\Facades\\Localization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
