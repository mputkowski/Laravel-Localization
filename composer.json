{
    "name": "mputkowski/laravel-locale",
    "description": "Powerful Localization for Laravel 5",
    "keywords": ["laravel", "framework", "localization", "locale", "Laravel Locale", "Laravel-Locale"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Putkowski",
            "email": "m.putkowski@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "autoload": {
        "psr-4": {
            "mputkowski\\Locale\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mputkowski\\Tests\\Locale\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "mputkowski\\Locale\\LocaleServiceProvider"
            ],
            "aliases": {
                "Locale": "mputkowski\\Locale\\Facades\\Locale"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
