{
    "name": "mputkowski/laravel-localization",
    "description": "Powerful Localization for Laravel",
    "keywords": [
        "laravel",
        "framework",
        "localization",
        "locale",
        "Laravel Localization",
        "Laravel-Localization",
        "Laravel Locale",
        "Laravel-Locale"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Michal Putkowski",
            "email": "michal@putkow.ski"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "illuminate/cookie": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.8|^10.0|^11.0",
        "graham-campbell/testbench": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "mputkowski\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mputkowski\\Tests\\Localization\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "mputkowski\\Localization\\ServiceProvider"
            ],
            "aliases": {
                "Localization": "mputkowski\\Localization\\Facades\\Localization"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}